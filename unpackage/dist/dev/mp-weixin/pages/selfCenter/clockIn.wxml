<view class="clockIn"><form data-event-opts="{{[['submit',[['submitFrom',['$event']]]]]}}" bindsubmit="__e"><view class="uni-form-item uni-column cl-time"><view class="form-item clockbar"><view class="clockImg"><image src="../../static/images/icon/clock.png" alt class="_img"></image></view><view class="itembtns timeBtns"><button class="btn" hover-class="default" loading="{{buttonStart}}" disabled="{{buttonStart}}" data-event-opts="{{[['tap',[['timingStart',['$event']]]]]}}" bindtap="__e">开始</button><button class="btn" hover-class="default" disabled="{{!buttonStart}}" data-event-opts="{{[['tap',[['stop',['$event']]]]]}}" bindtap="__e">完成</button></view></view><view class="form-item timetr"><text>记时：</text><text>{{times}}</text><text class="timesplit">-</text><text class="timetxt">{{startTime}}</text></view><view class="form-item timetr"><text>时长：</text><text>{{dayData}}</text></view></view><view class="uni-form-item uni-column"><view class="form-item nobtm mediaList"><view class="section"><image hidden="{{!(param.pictures)}}" class="imagetip" src="{{param.pictures}}"></image><view hidden="{{!(!param.pictures)}}" class="imagetip"><view class="imagetipicon"><text>+</text></view></view></view></view><view class="form-item nobtm itembtns"><button data-event-opts="{{[['tap',[['popUpImg',['$event']]]]]}}" class="btn" bindtap="__e">上传图片</button><button class="btn">上传视频</button><button class="btn">上传音频</button></view></view><view class="uni-form-item uni-column"><view class="title">描述</view><view class="form-item nobtm"><textarea class="inputarea" name="content" placeholder="请输入描述" maxlength="250"></textarea></view></view><view class="uni-form-item uni-column"><view class="title">概况</view><view class="form-item nobtm"><textarea class="inputarea" name="extendContent" placeholder="请输入概况" maxlength="250"></textarea></view></view><view class="btn_bar"><view class="btns"><button class="btn" form-type="submit">提交</button></view></view></form></view>